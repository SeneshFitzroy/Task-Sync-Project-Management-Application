{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter (Edge)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "deviceId": "chrome",
            "env": {
                "CHROME_EXECUTABLE": "C:\\Program Files (x86)\\Microsoft\\Edge\\Application\\msedge.exe"
            }
        },
        {
            "name": "Flutter (Web Server + Edge)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "deviceId": "web-server",
            "args": [
                "--web-port=8080"
            ]
        },
        {
            "name": "Flutter (Chrome)",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "deviceId": "chrome",
            "args": [
                "--web-port",
                "3000"
            ]
        },
        {
            "name": "Flutter (Windows)",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "deviceId": "windows"
        },
        {
            "name": "Flutter (Any Device)",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart"
        }
    ]
}
